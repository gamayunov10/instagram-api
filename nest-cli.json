{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/instagram-api/src",
  "compilerOptions": {
    "assets": [
      "features/mail/templates/**/*"
    ],
    "watchAssets": true,
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/instagram-api/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/instagram-api",
  "projects": {
    "instagram-api": {
      "type": "application",
      "root": "apps/instagram-api",
      "entryFile": "main",
      "sourceRoot": "apps/instagram-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/instagram-api/tsconfig.app.json"
      }
    },
    "files": {
      "type": "application",
      "root": "apps/files",
      "entryFile": "main",
      "sourceRoot": "apps/files/src",
      "compilerOptions": {
        "tsConfigPath": "apps/files/tsconfig.app.json"
      }
    }
  }
}